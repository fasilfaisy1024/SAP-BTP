<mvc:View displayBlock="true" controllerName="com.sapui5.zpurchase07.controller.DetailDetail" height="100%"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.f"
		xmlns:m="sap.m">

	<DynamicPage id="_IDGenDynamicPage1" toggleHeaderOnTitleClick="false">
		<title>
			<DynamicPageTitle id="_IDGenDynamicPageTitle1">
				<heading>
					<m:FlexBox id="_IDGenFlexBox1" wrap="Wrap" fitContainer="true" alignItems="Center">
						<m:Title id="_IDGenTitle1" text="{products>text}" wrapping="true" class="sapUiTinyMarginEnd"/>
					</m:FlexBox>
				</heading>

				<navigationActions>
					<m:OverflowToolbarButton id="_IDGenOverflowToolbarButton1" type="Transparent" icon="sap-icon://full-screen" press="handleFullScreen" tooltip="Enter Full Screen Mode" visible="{= ${/actionButtonsInfo/endColumn/fullScreen} !== null }"/>
					<m:OverflowToolbarButton id="_IDGenOverflowToolbarButton2" type="Transparent" icon="sap-icon://exit-full-screen" press="handleExitFullScreen" tooltip="Exit Full Screen Mode" visible="{= ${/actionButtonsInfo/endColumn/exitFullScreen} !== null }"/>
					<m:OverflowToolbarButton id="_IDGenOverflowToolbarButton3" type="Transparent" icon="sap-icon://decline" press="handleClose"  tooltip="Close column" visible="{= ${/actionButtonsInfo/endColumn/closeColumn} !== null }"/>
				</navigationActions>

			</DynamicPageTitle>
		</title>
		<content>
			<m:Link id="_IDGenLink1" text="Navigate" press="handleAboutPress" />
		</content>
	</DynamicPage>
</mvc:View>