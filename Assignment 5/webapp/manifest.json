{
  "_version": "1.58.0",
  "sap.app": {
    "id": "com.sapui5.zpurchase07",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/C_PURCHASEORDER_FS_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "fullWidth": true,
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.71.60",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.sapui5.zpurchase07.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.sapui5.zpurchase07.view",
        "controlId": "fcl"
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":layout:",
          "target": [
            "TargetMain"
          ]
        },
        {
          "name": "RouteDetail",
          "pattern": "detail/{product}/{layout}",
          "target": [
            "TargetMain",
            "TargetDetail"
          ]
        }
      ],
      "targets": {
        "TargetMain": {
          "controlAggregation": "beginColumnPages",
          "viewName": "Main"
        },
        "TargetDetail": {
          "controlAggregation": "midColumnPages",
          "viewName": "Detail"
        },
        "TargetDetaildetail": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "endColumnPages",
          "viewId": "Detaildetail",
          "viewName": "Detaildetail"
        }
      }
    },
    "rootView": {
      "viewName": "com.sapui5.zpurchase07.view.FlexibleColumnLayout",
      "type": "XML",
      "async": true,
      "id": "fcl"
    }
  }
}
